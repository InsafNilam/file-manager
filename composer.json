{
  "name": "laravelguru/laravel-filehandler",
  "description": "A powerful and easy-to-use file management package for Laravel, providing seamless file upload, storage, and retrieval functionalities with support for various storage drivers.",
  "keywords": [
    "laravelguru",
    "laravel-filehandler"
  ],
  "homepage": "https://github.com/laravelguru/laravel-filehandler",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Insaf Nilam",
      "email": "insafnilam.2000@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "LaravelGuru\\LaravelFilehandler\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelGuru\\LaravelFilehandler\\LaravelFilehandlerServiceProvider"
      ],
      "aliases": {
        "LaravelFilehandler": "LaravelGuru\\LaravelFilehandler\\LaravelFilehandlerFacade"
      }
    }
  },
  "minimum-stability": "stable"
}
