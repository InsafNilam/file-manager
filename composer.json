{
  "name": "laravelguru/laravel-filehandler",
  "description": "The Laravel Inertia React File Management package seamlessly integrates file management into your Laravel applications using React and Inertia.js. Featuring prebuilt file input components and popup file dialogs, this package simplifies file uploads, storage, browsing, and management.",
  "keywords": [
    "laravelguru",
    "laravel-filehandler",
    "laravel-filemanagement",
    "react-filemanagement",
    "inertia-filemanagement",
    "laravel-react-fileupload",
    "inertia-laravel-filehandler",
    "react-laravel-filehandling",
    "laravel-inertia-filemanagement",
    "react-fileupload-laravel",
    "inertia-fileupload-laravel",
    "react-laravel-filestorage",
    "inertia-laravel-filestorage",
    "react-filemanager-laravel",
    "laravel-inertia-fileupload",
    "laravel-react-filestorage",
    "inertia-laravel-fileuploader",
    "react-laravel-fileoperations",
    "inertia-filemanagement-react",
    "laravel-fileupload-component",
    "react-laravel-filebrowser",
    "inertia-laravel-filesystem",
    "react-laravel-filemanager"
  ],
  "homepage": "https://github.com/InsafNilam/file-manager.git",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Insaf Nilam",
      "email": "insafnilam.2000@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.3|~8.0.0|~8.1.0|~8.2.0|~8.3.0",
    "inertiajs/inertia-laravel": "^1.3.0",
    "laravel/framework": "^8.74|^9.0|^10.0|^11.0",
    "illuminate/support": "^10.0|^11.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "files": [
      "src/app/Http/Controllers/FileController.php",
      "src/app/Http/Resources/FileResource.php",
      "src/app/Models/File.php",
      "src/app/Services/FileManager.php",
      "src/app/Services/FileService.php"
    ],
    "psr-4": {
      "LaravelGuru\\LaravelFilehandler\\": "src",
      "LaravelGuru\\LaravelFilehandler\\App\\Http\\Controllers": "src/app/Http/Controllers",
      "LaravelGuru\\LaravelFilehandler\\App\\Http\\Resources": "src/app/Http/Resources",
      "LaravelGuru\\LaravelFilehandler\\App\\Models": "src/app/Models",
      "LaravelGuru\\LaravelFilehandler\\App\\Services": "src/app/Services"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LaravelGuru\\LaravelFilehandler\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "LaravelGuru\\LaravelFilehandler\\LaravelFilehandlerServiceProvider"
      ],
      "aliases": {
        "LaravelFilehandler": "LaravelGuru\\LaravelFilehandler\\LaravelFilehandlerFacade"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
